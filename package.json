{
  "private": true,
  "name": "storaji-website",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run -s serve || npm run -s dev",
    "build": "preact build --no-prerender --dest docs",
    "serve": "yarn build && preact serve",
    "dev": "preact watch",
    "lint": "eslint src"
  },
  "eslintConfig": {
    "extends": "eslint-config-synacor"
  },
  "eslintIgnore": [
    "build/*",
    "docs/*"
  ],
  "devDependencies": {
    "eslint": "^4.9.0",
    "eslint-config-synacor": "^2.0.2",
    "if-env": "^1.0.0",
    "node-sass": "^4.7.2",
    "preact-cli": "^2.0.1",
    "sass-loader": "^6.0.6"
  },
  "dependencies": {
    "json-loader": "^0.5.7",
    "preact": "^8.2.6",
    "preact-compat": "^3.17.0",
    "preact-router": "^2.5.7",
    "ua-parser-js": "^0.7.17",
    "uikit": "~3.0.0-beta.35"
  }
}
